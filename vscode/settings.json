{
  // Place your settings in this file to overwrite the default settings
  "editor.fontSize": 14,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.foldingStrategy": "indentation",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "vs-seti",
  "editor.rulers": [80],
  "editor.minimap.enabled": true,
  "editor.renderIndentGuides": true,
  "editor.showFoldingControls": "always",
  "workbench.sideBar.location": "left",
  "window.title": "${activeEditorLong}",
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.exclude": {
    "**/dest": true,
    "**/dist": true,
    "**/log": true,
    "**/logs": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/reports": true,
    "**/.nyc_output": true,
    "**/.cache": true,
    "**/.out": true,
    "**/vendor": true,
    "**/pacts": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/*.meta": true,
    "**/*.*.meta": true,
    "**/*.unity": true,
    "**/*.unityproj": true,
    "**/*.mat": true,
    "**/*.fbx": true,
    "**/*.FBX": true,
    "**/*.tga": true,
    "**/*.cubemap": true,
    "**/*.prefab": true,
    "**/Library": true,
    "**/ProjectSettings": true,
    "**/Temp": true
  },
  "files.watcherExclude": {
    "**/dest/**": true,
    "**/dist/**": true,
    "**/log/**": true,
    "**/logs/**": true,
    "**/bower_components/**": true,
    "**/node_modules/**": true,
    "**/coverage/**": true,
    "**/reports/**": true,
    "**/.nyc_output/**": true,
    "**/.cache/**": true,
    "**/.out/**": true,
    "**/vendor/**": true,
    "**/pacts/**": true
  },
  "search.exclude": {
    "**/dest": true,
    "**/dist": true,
    "**/log": true,
    "**/logs": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/coverage": true,
    "**/reports": true,
    "**/.nyc_output": true,
    "**/.cache/**": true,
    "**/.out/**": true,
    "**/vendor/**": true,
    "**/pacts/**": true
  },
  "css.lint.duplicateProperties": "error",
  "html.format.endWithNewline": true,
  "files.trimTrailingWhitespace": true,
  "window.zoomLevel": 0,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 9000,
  "css.validate": false,
  "scss.validate": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.keymap": "alternate",
  "editor.snippetSuggestions": "top",
  "breadcrumbs.enabled": true,
  "workbench.settings.editor": "json",
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.highlightModifiedTabs": true,
  "gitlens.views.repositories.files.layout": "auto",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "editor.suggestSelection": "first",
  "workbench.colorTheme": "Oceanic Next",
  "[javascript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "workbench.statusBar.visible": true,
  "local-history.path": "/Users/{userName}/.vscode/.local-history",
  "local-history.treeLocation": "localHistory",
  "local-history.daysLimit": 5,
  "diffEditor.ignoreTrimWhitespace": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5"
}
